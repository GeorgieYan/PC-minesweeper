#include <stdio.h>
#include <stdlib.h>


int field[100][100];
int rows, columns;

int main(){
    int field_number = 1;
    int i, j, m, n;

    scanf("%d %d", &i, &j);

    for(;;){
        if(i > 100||j > 100){
            break;
        }

        default_field(i, j);

        for(m = 0; m < i; m++){
            char line[j + 1];
            scanf("%s", line);
            for(n = 0; n < j; n++){
                char mine_detector = line[j];
                if(mine_detector == '*'){
                    plant(i, j);
                }
            }
        }

        print(field_number);

        field_number++;
    }
}

void print(field_number){
    printf("Field #%d:\n", field_number);

    int i, j;
    for(i = 0; i < rows; i++){
        for(j = 0; j < columns; j++){
            int x = field[i][j];

            if(x == -1){
                printf("*");
            }
            else{
                printf("%d", x);
            }
        }
        printf("\n");
    }
}

void default_field(i, j){
    rows = i;
    columns = j;
    int m, n;
    for(m = 0; m < rows; m++){
        for(n = 0; n < columns; n++){
            field[m][n] = 0;
        }
    }
}

void plant(int i, int j){
    field[i][j] = -1;

    int m, n;

    int t, p;
    for(t = 0; t < rows; t++){
        for(p = 0; p < columns; p++){
            if(field[t][p] = -1){
                for(m = -1; m < 2; m++){
                    for(n = -1; n < 2; n++){
                        field[i + m][j + n]++;
                    }
                }
            }
        }
    }

}
